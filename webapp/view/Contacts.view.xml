<mvc:View
	xmlns="sap.m"
	xmlns:mvc="sap.ui.core.mvc"
	xmlns:l="sap.ui.layout"
	xmlns:f="sap.ui.layout.form"
	xmlns:core="sap.ui.core"
	controllerName="sap.ui.iba.practic.controller.Contacts">
	<Page
		title = "{i18n>contacts}"
		showNavButton="true"
		navButtonPress="onNavBack">
		<VBox class="sapUiSmallMargin">
			<f:SimpleForm id="SimpleFormDisplay354wideDual"
				editable="false"
				layout="ResponsiveGridLayout"
				title="{i18n>adress}"
				labelSpanXL="4"
				labelSpanL="3"
				labelSpanM="4"
				labelSpanS="12"
				adjustLabelSpan="false"
				emptySpanXL="0"
				emptySpanL="4"
				emptySpanM="0"
				emptySpanS="0"
				columnsXL="2"
				columnsL="1"
				columnsM="1"
				singleContainerFullSize="false" >
				<f:content>
					<core:Title text="{i18n>office}" />
					<Label text="{i18n>fName}" />
					<Text id="nameText" text="{i18n>mName}" />
					<Label text="{i18n>fUniversity}" />
					<Text text="{i18n>mUniversity}" />
					<Label text="{i18n>fCity}" />
					<Text text="{i18n>mCity}" />
					<Label text="{i18n>fCountry}" />
					<Text id="countryText" text="{i18n>mCountry}" />
					<core:Title text="{i18n>online}" />
					<Label text="{i18n>fEmail}" />
					<Text text="{i18n>mEmail}" />
					<Label text="{i18n>fInst}" />
					<Text text="{i18n>mInst}" />
				</f:content>
			</f:SimpleForm>
		</VBox>
	</Page>
</mvc:View>